globals:
    requirejs: true
env:
    browser: true
    amd: true
rules:
    no-unserscore-dangle:
        - 2
    quotes:
        - 2
        - single
    brace-style:
        - 2
        - stroustrup
        - allowSingleLine: true
    func-style:
        - 2
        - declaration
    consistent-this:
        - 0
        - self
